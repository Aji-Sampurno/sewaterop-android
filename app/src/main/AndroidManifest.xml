<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sewaterop">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApplication1">
        <activity
            android:name="com.example.sewaterop.SplashScreen"
            android:label="@string/title_activity_splash_screen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.sewaterop.RegistrasiActivity"
            android:label="@string/title_activity_registrasi"></activity>
        <activity android:name="com.example.sewaterop.LoginActivity"></activity>
        <activity
            android:name="com.example.sewaterop.MainActivity"
            android:label="@string/app_name"></activity>
    </application>

</manifest>